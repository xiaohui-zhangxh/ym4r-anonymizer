require 'rubygems'
require 'rake'
require 'echoe'

Echoe.new('ym4r-anonymizer', '0.0.1') do |p|
  p.description = "An extension of Ym4r, allow us geocode address with proxy and automatically iterate API keys"
  p.url         = "http://github.com/xiaohui-zhangxh/ym4r-anonymizer"
  p.author      = "Zhang, Xiaohui"
  p.email       = "xiaohui@zhangxh.net"
  p.ignore_pattern = ["test/*"]
  p.runtime_dependencies = ["ym4r > 0"]
end

